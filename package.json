{
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "electron": "^16.0.6",
    "electron-packager": "^15.4.0",
    "electron-reloader": "^1.2.1",
    "typescript": "^4.5.4"
  },
  "name": "docker-tray",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "babel src --extensions \".js,.ts\"  --watch --out-dir dist --copy-files",
    "start": "electron .",
    "build": "yarn compile && electron-packager . docker-manager --platform=linux --arch=x64"
  }
}
